{
  "name": "yelp-search",
  "version": "1.0.0",
  "description": "Tinder for food",
  "main": "index.js",
  "scripts": {
    "start": "webpack -d --watch; node server/index.js; node-sass --include-path scss ./client/src/main.scss",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "start:dev": "npm run webpack -- -w & nodemon --ignore src/ --ignore dist/ server/index.js",
    "server-dev": "nodemon server/index.js",
    "react-dev": "webpack -d --watch",
    "sass-dev": "node-sass -w ./client/src/main.scss -o ./client/dist/",
    "build-css": "node-sass --include-path scss ./client/src/main.scss ./client/dist/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "standard": "standard index.js",
    "test": "ava spec/mongo-ava-spec.js"
  },
  "author": "CoolStuffIncorporated",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "co-exec": "^1.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "ghooks": "^1.2.4",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "nyc": "^10.3.2",
    "sass-loader": "^7.0.3",
    "sinon": "^5.1.0",
    "standard": "^8.4.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "materialize-css": "^0.100.2",
    "mongodb-memory-server": "^1.8.0",
    "mongoose": "^5.1.3",
    "morgan": "^1.9.0",
    "react": "^16.4.0",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.87.0",
    "sass": "^1.5.1",
    "scss": "^0.2.4",
    "supertest": "^3.1.0",
    "yelp-fusion": "^2.0.3"
  },
  "bugs": {
    "url": "https://github.com/CoolStuffIncorporated/yelp-search/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoolStuffIncorporated/yelp-search.git"
  },
  "homepage": "https://github.com/CoolStuffIncorporated/yelp-search",
  "config": {
    "ghooks": {
      "pre-commit": "npm run standard"
    }
  },
  "keywords": [
    "yelp-fusion",
